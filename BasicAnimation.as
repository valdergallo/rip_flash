package{    import flash.events.Event;    import flash.events.MouseEvent;    import flash.display.Sprite;    public class BasicAnimation extends Sprite    {        // Child display objects        private var _rect:Sprite;        // Private constants        private const START:int = 10;        private const END:int = 400;        // construction        public function BasicAnimation()        {            _createChildren();			trace('ROCK');        }        // Private methods        private function _createChildren():void        {            _rect = new Sprite();            _rect.graphics.beginFill(0x000000, 1);            _rect.graphics.drawRect(0, 0, 100, 100);            _rect.graphics.endFill();            _rect.x = START;			_rect.y = 100;            _rect.buttonMode = true;			_rect.doubleClickEnabled = true;            addChild(_rect);            _rect.addEventListener(MouseEvent.CLICK, _rect_click);            _rect.addEventListener(MouseEvent.DOUBLE_CLICK, _rect_double_click);        }        // Event handling        private function _rect_click(event:MouseEvent):void        {			trace('Click');            addEventListener(Event.ENTER_FRAME, _enterFrameBasic);        }        // Event handling        private function _rect_double_click(event:MouseEvent):void        {			trace('Double Click');            removeEventListener(Event.ENTER_FRAME, _enterFrameBasic);        }        private function _enterFrameBasic(event:Event):void        {            if (_rect.x < END){                _rect.x++;            }else{				_rect.x = START                removeEventListener(Event.ENTER_FRAME, _enterFrameBasic);            }        }    }}